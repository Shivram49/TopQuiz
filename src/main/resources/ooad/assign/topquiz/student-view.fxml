<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="459.0" prefWidth="548.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ooad.assign.topquiz.controllers.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label prefHeight="45.0" prefWidth="232.0" style="-fx-alignment: center;" text="Take Quiz" textFill="#bc1212">
      <font>
         <Font name="Times New Roman" size="34.0" />
      </font>
   </Label>
   <TextField fx:id="username" pickOnBounds="false" prefHeight="26.0" prefWidth="396.0" promptText="Username" />
   <PasswordField fx:id="password" promptText="password" />
    <Button fx:id="takeQuiz" prefHeight="58.0" prefWidth="181.0" style="-fx-alignment: center; -fx-background-radius: 360;" text="Press Here" textFill="#b90f0f">
      <font>
         <Font name="Stencil" size="18.0" />
      </font></Button>
</VBox>
